{
    "name": "diegonz/laroute",
    "description": "Access Laravel's URL/Route helper functions, from JavaScript.",
    "keywords": [
        "laravel",
        "routes",
        "routing",
        "javascript"
    ],
    "homepage": "https://github.com/diegonz/laroute",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Aaron Lord",
            "email": "hello@aaronlord.is"
        },
        {
            "name": "Diego GonzÃ¡lez",
            "email": "dev.diegonz@gmail.com",
            "homepage": "https://github.com/diegonz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": ">=5.0.0, <5.9.0",
        "illuminate/routing": ">=5.0.0, <5.9.0",
        "illuminate/console": ">=5.0.0, <5.9.0",
        "illuminate/config": ">=5.0.0, <5.9.0",
        "illuminate/filesystem": ">=5.0.0, <5.9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.8.*",
        "mockery/mockery": "dev-master",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Lord\\Laroute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lord\\Laroute\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lord\\Laroute\\LarouteServiceProvider"
            ]
        }
    }
}
